all: 
	make clean 
	make klingon
	make rmObj

clean:
	!Rm *.o
	!Rm render

rmObj:
	!Rm *.o
	
	
klingon: klingon.o renderer.o model.o events.o p1_ship.o en_ship.o raster.o fat.o empty.o clr_qk.o
	cc68x klingon.o renderer.o model.o events.o p1_ship.o en_ship.o raster.o fat.o empty.o clr_qk.o -o klingon

klingon.o: klingon.c model.h renderer.h events.h globals.h 
	cc68x -g -c klingon.c

renderer.o: renderer.c renderer.h globals.h
	cc68x -g -c renderer.c
	
model.o: model.c model.h globals.h
	cc68x -g -c model.c
	
events.o: events.c events.h globals.h
	cc68x -g -c events.c
	
p1_ship.o: p1_ship.c p1_ship.h globals.h
	cc68x -g -c p1_ship.c
	
en_ship.o: en_ship.c en_ship.h globals.h
	cc68x -g -c en_ship.c
	
raster.o: raster.c raster.h globals.h
	cc68x -g -c raster.c
	
fat.o: fat.c fat.h globals.h
	cc68x -g -c fat.c
	
empty.o: empty.c empty.h globals.h
	cc68x -g -c empty.c
	
clr_qk.o: clr_qk.s clr_qk.h
	gen -L2 clr_qk.s
	